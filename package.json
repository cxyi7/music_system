{
  "name": "music_system",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "commit": "git-cz",
    "lint": "eslint --ext .vue,.js,.jsx,.ts,.tsx ./src/  --max-warnings 0",
    "lint:fix": "eslint --ext .vue,.js,.jsx,.ts,.tsx ./src/ --max-warnings 0 --fix",
    "stylelint": "stylelint src/**/*.{html,vue,sass,less}",
    "stylelint:fix": "stylelint src/**/*.{html,vue,sass,less} --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^9.1.0",
    "pinia": "^2.0.20",
    "tdesign-vue-next": "^0.20.4",
    "vue": "^3.2.37",
    "vue-router": "^4.1.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@commitlint/cz-commitlint": "^17.1.2",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@vitejs/plugin-vue": "^3.0.3",
    "autoprefixer": "^10.4.8",
    "commitizen": "^4.2.5",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.4.0",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.16",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.7.1",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-less": "^1.0.6",
    "stylelint-order": "^5.0.0",
    "terser": "^5.15.0",
    "typescript": "^4.8.2",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^3.0.7",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-windicss": "^1.8.7",
    "windicss": "^3.5.6"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,vue,ts,tsx}": [
      "prettier --write",
      "npm run lint:fix"
    ],
    "src/**/*.{html,vue,sass,less}": [
      "npm run stylelint:fix"
    ]
  }
}
